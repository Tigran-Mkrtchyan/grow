        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
    http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
        version="2.0">

    <entity class="com.grow.workshop_core.domain.aggregate.WorkshopAggregate"
            access="FIELD" name="WorkshopAggregate">
        <table name="workshops_v2"/>
        <attributes>
           <basic name="hostEmail">
