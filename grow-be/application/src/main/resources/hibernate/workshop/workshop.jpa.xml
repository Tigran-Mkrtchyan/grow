            <basic name="hostName">
                <column name="host_name"/>
            </basic>
            <basic name="title">
                <column name="title"/>
            </basic>
            <basic name="hostEmail">
                <column name="host_email"/>
            </basic>
            <basic name="reporterId">
                <column name="reporter_id"/>
